            ],
            "fields": [

            ],
            "methods": [
                {
                    "name": "size",
                    "comment_text": "{@inheritDoc}\n\n <p>This implementation returns <tt>entrySet().size()</tt>.",
                    "return_type": "int",
                    "parameters": [

                    ],
                    "throws": [

                    ]
                },
                {
